<Window x:Class="AutoUpdate.WpfApp_Net6.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutoUpdate.WpfApp_Net6"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <ProgressBar Grid.Row="1" Height="30" Width="500" Value="{Binding ProgressValue}" Minimum="{Binding ProgressMinValue}" Maximum="{Binding ProgressMaxValue}"/>
        <StackPanel Grid.Row="2" Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center">
            <TextBlock Text="Seep："></TextBlock>
            <TextBlock Text="123"></TextBlock>
            <TextBlock Text="123"></TextBlock>
            <TextBlock Text="123"></TextBlock>
            <TextBlock Text="123"></TextBlock>
            <TextBlock Text="123"></TextBlock>
        </StackPanel>
    </Grid>
</Window>
