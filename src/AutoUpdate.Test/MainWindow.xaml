<Window x:Class="AutoUpdate.Test.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutoUpdate.Test"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
        <Button Width="100" Height="30" Content="Update" Click="Button_Click" Margin="5"></Button>
        <Button Width="100" Height="30" Content="MD5" Click="Button_Click_1" Margin="5"></Button>
        <TextBox Width="200" Name="TxtMD5" Height="25" Margin="5"></TextBox>
    </StackPanel>
</Window>
